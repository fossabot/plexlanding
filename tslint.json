{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "no-boolean-literal-compare": false,
    "strict-boolean-expressions": false,
    "no-unused-variable": true,
    "import-name": false
  },
  "rulesDirectory": []
}
